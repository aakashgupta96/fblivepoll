  <script>
    //Hack to show tabs correctly with responsve nature
    $( document ).ready(function(){
        setTimeout(function(){  
          $(".tabs li")[1].children[0].click();
          setTimeout(function(){
            $(".tabs li")[0].children[0].click();
          },250);
        },1);
    });

    var resizeFuntion = function(elements)
    {
      var sumHeight = 0;
      for(var i=0;i<elements.length;i++)
      {
        //console.log(" i = " + i + " height is = " + $(elements[i]).height());
        sumHeight+= $(elements[i]).height();
      }
      $(".carousel").animate({height: Math.max(sumHeight+60,400)},300);
    }
    var resizeCarouselFromAnchor = function(e){
      var elements = $($(this).attr('href') + " li");
      resizeFuntion(elements);
    };

    var resizeCarouselFromHeader = function(e){
      var elements = $(this).parent().parent()[0].getElementsByTagName("li");
      setTimeout(function(){resizeFuntion(elements)},300);
    };

    $(".activate-resize").click(resizeCarouselFromAnchor);
    
    $(".collapsible-header").click(resizeCarouselFromHeader);

  </script>